{
  "name": "108-yasumi-kanri",
  "version": "1.0.0",
  "description": "休み管理",
  "main": "dist/customize.js",
  "scripts": {
    "envexample": "generate-env-example",
    "dtsgen:yasumi": "dotenv -- npx kintone-dts-gen --app-id 108 --namespace Yasumi -o types/yasumi.d.ts",
    "tsc": "tsc --noEmit --project tsconfig.json",
    "cypress:open": "cypress open",
    "upload:local": "dotenv -- npx kintone-customize-uploader dest/kintone.local.json",
    "upload:prod": "dotenv -- npx kintone-customize-uploader dest/kintone.prod.json",
    "test": "jest --verbose ./src/__TEST__",
    "dev": "esbuild src/index.ts --bundle --define:process.env.NODE_ENV='development' --minify=false --loader:.png=dataurl --outfile=dist/customize.js  --watch",
    "localprod": "esbuild src/index.ts --bundle --define:process.env.NODE_ENV='localproduction' --minify=false --loader:.png=dataurl --outfile=dist/customize.js --watch",
    "build": "esbuild src/index.ts --bundle --define:process.env.NODE_ENV='production' --minify=true --loader:.png=dataurl --outfile=dist/customize.js",
    "deploy": "npm run build"
  },
  "author": "Lorenz Ras",
  "license": "ISC",
  "devDependencies": {
    "@kintone/dts-gen": "^8.0.3",
    "@tanstack/eslint-plugin-query": "^5.17.1",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.6",
    "@types/qs": "^6.9.11",
    "@types/react": "^18.2.70",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "cypress": "^13.6.2",
    "esbuild": "0.19.11",
    "eslint": "^8.56.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-react": "^0.27.0",
    "eslint-config-xo-typescript": "^1.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "generate-env-example": "^1.1.0",
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "prettier": "3.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "xo": "^0.56.0"
  },
  "dependencies": {
    "@babel/preset-env": "^7.23.7",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fullcalendar/core": "^6.1.10",
    "@fullcalendar/daygrid": "^6.1.10",
    "@fullcalendar/interaction": "^6.1.10",
    "@fullcalendar/react": "^6.1.10",
    "@kintone/customize-uploader": "^8.0.6",
    "@kintone/rest-api-client": "^5.0.5",
    "@mui/icons-material": "^5.15.3",
    "@mui/material": "^5.15.3",
    "@tanstack/react-query": "^5.17.9",
    "@tanstack/react-query-devtools": "^5.17.9",
    "axios": "^1.6.5",
    "babel-jest": "^29.7.0",
    "date-fns": "^3.1.0",
    "dotenv": "^16.3.1",
    "jotai": "^2.6.1",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.1.1"
  }
}
